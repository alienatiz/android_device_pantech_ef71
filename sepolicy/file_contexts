/sys/devices(/platform)?/soc/soc:gpio-leds/leds(/.*)?                                   u:object_r:sysfs_leds:s0

# VirtualKeys
/sys/board_properties/virtualkeys.*         u:object_r:sysfs_virtualkeys:s0

# TimeKeep
/sys/devices/soc/qpnp-rtc-8/rtc/rtc0(/.*)?                         u:object_r:sysfs_rtc:s0
